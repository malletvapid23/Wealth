{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": ".",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [ "es2015", "dom" ],
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "paths": {
            "environment-settings": [ "app/main/settings/setup/environment-settings" ]
        },
        "removeComments": true,
        "sourceMap": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "files": [
        "app/boot.ts"
    ],
    "angularCompilerOptions": {
        "genDir": "aot",
        "skipMetadataEmit": true
    }
}
